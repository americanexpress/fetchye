  },
  "peerDependencies": {
    "react": ">=16.14.0",
    "react-dom": ">=16.14.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "7.11.6",
    "@testing-library/react": "^11.0.4",
    "babel-preset-amex": "^3.4.1",
    "cross-env": "^7.0.2",
    "fetchye-test-utils": "^1.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
