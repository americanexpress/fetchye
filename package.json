    "immutable": "^4.0.0-rc.12",
    "jest": "^26.4.0",
    "lockfile-lint": "^4.3.7",
    "react": "16.x.x",
    "react-dom": "16.x.x",
    "react-redux": "7.x.x",
    "redux": "4.x.x",
    "semantic-release": "^17.1.1"
