  "peerDependencies": {
    "react": "16.x.x",
    "react-dom": "16.x.x",
    "react-redux": "7.x.x",
    "redux": "4.x.x"
  },
  "optionalDependencies": {
    "immutable": "^4.0.0-rc.12"
  },
